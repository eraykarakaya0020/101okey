{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/monke/(.*)", "dest": "/api/monke/$1" },
    { "src": "/function/(.*)", "dest": "/api/monke/function/$1" },
    { "src": "/(.*)", "dest": "/api/$1" }
  ],
  "env": {
    "PHP_VERSION": "8.2"
  }
}